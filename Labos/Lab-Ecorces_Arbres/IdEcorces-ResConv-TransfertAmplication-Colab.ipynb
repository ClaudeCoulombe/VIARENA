      "metadata": {
        "id": "7878c7f4"
      },
      "outputs": [],
      "source": [
        "# Création d'un flux de données pour les tests\n",
        "donnees_test = tf.keras.utils.image_dataset_from_directory(REPERTOIRE_TEST,\n",
        "                                                           batch_size=TAILLE_LOT,\n",
        "                                                           image_size=TAILLE_IMAGE)"
